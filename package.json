{
  "name": "guidesmiths-challenge",
  "version": "1.0.0",
  "description": "Marte code challenge",
  "scripts": {
    "test": "NODE_ENV=test ts-mocha -p tsconfig.json --opts ./mocha.opts --timeout 10000 --exit",
    "serve": "npm run build-ts & npm run start",
    "delete-dist": "rm -rf dist",
    "copy-interfaceValidators": "cp -r src/types/interface-validator/json-schemas dist/src/types/interface-validator/",
    "copy-config": "cp -R ./config/ ./dist/",
    "build-ts": "npm run delete-dist && npm run build",
    "build": "tsc -p tsconfig.build.json && npm run copy-config && npm run copy-interfaceValidators",
    "create": "npm run build-ts && npm run test",
    "install:cli": "sudo npm link",
    "start": "NODE_ENV=dev node dist/src/main.js",
    "start:dev": "NODE_ENV=dev nodemon"
  },
  "main": "./dist/src/index.js",
  "bin": {
    "mars": "./dist/src/index.js"
  },
  "author": "Enrique Rubio",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.9",
    "@types/fs-extra": "^9.0.5",
    "ajv": "^6.12.6",
    "body-parser": "^1.19.0",
    "config": "^3.2.2",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.3.3",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.7",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "helmet": "^4.2.0",
    "inquirer": "^7.3.3",
    "inversify": "^5.0.5",
    "kleur": "^4.1.3",
    "logger": "0.0.1",
    "morgan": "^1.8.2",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.0.3",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.1.0"
  },
  "devDependencies": {
    "@liftr/tscov": "^2.0.0",
    "@types/body-parser": "^1.17.1",
    "@types/chai": "^4.2.1",
    "@types/chai-as-promised": "^7.1.2",
    "@types/figlet": "^1.2.1",
    "@types/inquirer": "^7.3.1",
    "@types/lodash": "^4.14.71",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^5.5.17",
    "@types/node": "^13.13.35",
    "@types/sinon": "^7.0.13",
    "axios": "^0.21.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "lodash": "^4.17.20",
    "mocha": "^6.2.0",
    "nock": "^11.3.4",
    "nodemon": "^1.19.2",
    "prettier": "^1.15.3",
    "qs": "^6.9.4",
    "sinon": "^7.4.2",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.10.2",
    "tsc-watch": "^1.0.7",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-etc": "^1.7.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typedoc": "^0.19.2",
    "typescript": "^3.9.7"
  }
}
